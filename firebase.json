{
  "hosting": {
    "site": "asker-platform1",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "nextjs-app-platform",
          "region": "europe-west3"
        }
      }
    ]
  },
  "functions": {
    "source": "cloud-functions"
  }
}
